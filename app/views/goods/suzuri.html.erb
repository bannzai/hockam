<h1>Goods#suzuri</h1>
<% @suzuri_goods.each do |a| %> 
  <div class="square">
    <div class="table">
      <div class="table-cell">
        <div class="bg-image", style="background-image: url('<%= a.list_image_url =%>');"></div>
        <p class="goods-name"> <%= a.name =%> </p>
      </div>
    </div>
  </div>
<% end %> 
<div style='clear:left;'> 
  <%= paginate @suzuri_goods %>
</div>
